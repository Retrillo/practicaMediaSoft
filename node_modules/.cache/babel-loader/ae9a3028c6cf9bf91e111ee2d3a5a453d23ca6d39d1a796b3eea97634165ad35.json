\u0441\\u043E\\u043A\\u043E\\u043A\\u0430\\u0447\\u0435\\u0441\\u0442\\u0432\\u0435\\u043D\\u043D\\u044B\\u0435 \\u0442\\u043E\\u0432\\u0430\\u0440\\u044B, \\u043D\\u043E \\u0438 \\u043F\\u043E\\u043C\\u043E\\u0449\\u044C \\u0432 \\u0438\\u0445 \\u0443\\u0441\\u0442\\u0430\\u043D\\u043E\\u0432\\u043A\\u0435, \\u043E\\u0431\\u0441\\u043B\\u0443\\u0436\\u0438\\u0432\\u0430\\u043D\\u0438\\u0438 \\u0438 \\u0440\\u0435\\u043C\\u043E\\u043D\\u0442\\u0435. \\u041D\\u0430\\u0448\\u0430 \\u043A\\u043E\\u043C\\u0430\\u043D\\u0434\\u0430 \\u043F\\u0440\\u043E\\u0444\\u0435\\u0441\\u0441\\u0438\\u043E\\u043D\\u0430\\u043B\\u043E\\u0432 \\u0432\\u0441\\u0435\\u0433\\u0434\\u0430 \\u0433\\u043E\\u0442\\u043E\\u0432\\u0430 \\u043F\\u0440\\u0435\\u0434\\u043E\\u0441\\u0442\\u0430\\u0432\\u0438\\u0442\\u044C \\u0432\\u0430\\u043C \\u043A\\u0432\\u0430\\u043B\\u0438\\u0444\\u0438\\u0446\\u0438\\u0440\\u043E\\u0432\\u0430\\u043D\\u043D\\u0443\\u044E \\u043A\\u043E\\u043D\\u0441\\u0443\\u043B\\u044C\\u0442\\u0430\\u0446\\u0438\\u044E \\u0438 \\u043F\\u043E\\u043C\\u043E\\u0447\\u044C \\u0432\\u044B\\u0431\\u0440\\u0430\\u0442\\u044C \\u043D\\u0430\\u0438\\u0431\\u043E\\u043B\\u0435\\u0435 \\u043F\\u043E\\u0434\\u0445\\u043E\\u0434\\u044F\\u0449\\u0443\\u044E \\u043C\\u043E\\u0434\\u0435\\u043B\\u044C. \\u041A\\u0440\\u043E\\u043C\\u0435 \\u0442\\u043E\\u0433\\u043E, \\u043C\\u044B \\u0430\\u043A\\u0442\\u0438\\u0432\\u043D\\u043E \\u0437\\u0430\\u043D\\u0438\\u043C\\u0430\\u0435\\u043C\\u0441\\u044F \\u0443\\u043B\\u0443\\u0447\\u0448\\u0435\\u043D\\u0438\\u0435\\u043C \\u044D\\u043A\\u043E\\u043B\\u043E\\u0433\\u0438\\u0447\\u0435\\u0441\\u043A\\u043E\\u0439 \\u0441\\u043E\\u0441\\u0442\\u0430\\u0432\\u043B\\u044F\\u044E\\u0449\\u0435\\u0439 \\u043D\\u0430\\u0448\\u0435\\u0439 \\u0434\\u0435\\u044F\\u0442\\u0435\\u043B\\u044C\\u043D\\u043E\\u0441\\u0442\\u0438. \\u041C\\u044B \\u043F\\u043E\\u043D\\u0438\\u043C\\u0430\\u0435\\u043C, \\u043D\\u0430\\u0441\\u043A\\u043E\\u043B\\u044C\\u